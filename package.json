{
  "name": "formmanager",
  "version": "1.0.0-alpha.1",
  "description": "Manages forms",
  "main": "dist/formmanager.js",
  "keywords": [
    "browser",
    "forms",
    "manage"
  ],
  "scripts": {
    "dev": "nswatch",
    "build": "scriptkit",
    "test": "browserify -t [ babelify --presets [ es2015 ] ] test/formmanager.test.js | tape-run --static . --browser phantomjs --render tap-spec",
    "lint": "eslint . --ignore-pattern dist",
    "prepublish": "npm run build"
  },
  "watch": {
    "lib/*.js": [
      "lint",
      "build",
      "test"
    ],
    "test/*.js": [
      "lint",
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/formmanager.git"
  },
  "author": "First+Third",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/formmanager/issues"
  },
  "homepage": "https://github.com/firstandthird/formmanager#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "domodule": "^4.0.0",
    "eslint": "^3.9.1",
    "eslint-config-firstandthird": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "nswatch": "^0.2.0",
    "phantomjs": "^2.1.7",
    "scriptkit": "0.0.10",
    "static-server": "^2.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scriptkit": {
    "scripts": {
      "dist/formmanager.js": "lib/formmanager.js"
    }
  },
  "dependencies": {
    "@firstandthird/formbuilder": "^1.0.0-beta.7",
    "bequest": "^1.3.1",
    "formobj": "^2.0.0"
  }
}
